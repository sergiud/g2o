cmake_policy (VERSION 3.1)

@PACKAGE_INIT@

include (CMakeFindDependencyMacro)

@Eigen3_DEPENDENCY@
@g2o_DEPENDENCY@
@CXSparse_DEPENDENCY@

include ("${CMAKE_CURRENT_LIST_DIR}/g2o-targets.cmake")

set (_g2o_NAMESPACE "@g2o_NAMESPACE@")

foreach (_comp ${g2o_FIND_COMPONENTS})
  set (_TARGET ${_g2o_NAMESPACE}${_comp})

  if (TARGET ${_TARGET})
    set (g2o_${_comp}_FOUND TRUE)
  else (TARGET ${_TARGET})
    set (g2o_${_comp}_FOUND FALSE)
    set (g2o_NOT_FOUND_MESSAGE "g2o could not be found because the component ${_comp} could not be found.")
  endif (TARGET ${_TARGET})
endforeach (_comp)

check_required_components (g2o)
